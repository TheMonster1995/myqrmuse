    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:tailwind": "tailwindcss -o src/tailwind.css",
    "watch:tailwind:dev": "tailwindcss -o src/tailwind.css -w",
    "start": "run-p watch:tailwind:dev start:react",
    "start:react": "react-scripts start",
    "build": "run-s watch:tailwind build:react",
    "build:react": "react-scripts build",
    "lint": "npm run tsc-check && npm run eslint",
